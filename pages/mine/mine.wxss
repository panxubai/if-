/* pages/mine/mine.wxss */
page{
  background: #edf2f5;
}
.container{
  width: 100%;
}
.mainSum{
  width: 100%;
  height: 430rpx;
  background-image: url(data:image/jpg;base64,);
  background-size:contain;
   border-top: 1px solid #00b65f;
   position: relative;
}
.mainSum .name{
  margin: 120rpx auto 30rpx;
  font-weight: 600;
  font-size: 34rpx;
  color:#fff; 
  text-align: center;
  
}
.mainSum .headImg{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin: 0 auto;
  border: 5rpx solid #fff;
  box-shadow: 0px 5rpx 5rpx 0px #00b65f;
  position: relative;
  z-index: 5;
}
.mainSum .headImg .login{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 150rpx;
  background: #edf2f5;
  font-size: 35rpx;
  color: #b4b4b6;
}
.mainSum .headImg image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.list{
  width: 710rpx;
  height: 240rpx;
  margin: 0 auto;
  border-radius: 30rpx;
  background: #fff;
  box-shadow:0px 4rpx 5rpx #00b65f;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: relative;
  bottom: 40rpx;
  z-index: 3;
}
.list .listLi{
flex: 1;
text-align: center;
  margin-top: 98rpx;
}
.list .listLi image{
  width: 45rpx;
  height: 45rpx;
}
/* .list .listLi:nth-child(3) image{
  width:37rpx;
  height:45rpx;
}
.list .listLi:nth-child(4) image{
  width:49rpx;
  height:45rpx;
} */
.list .listLi text{
  display: block;
  width: 100%;
  text-align: center;
font-size: 24rpx;
}
.bottomBox{
  width: 100%;
  margin-top: 400rpx;
}
.bottomBox .bottomSum{
   width: 90%;
   margin:  0 auto;
}
.bottomBox .bottomSum .bshare{
  width: 100%;
  height: 55rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 25rpx;
}
.bottomBox .bottomSum .bshare .bImg{
  width: 40rpx;
  height: 40rpx;
}
.bottomBox .bottomSum .bshare .bImg image{
    width: 40rpx;
  height: 40rpx;
}
.bottomBox .bottomSum .bshare .bText{
flex: 1;
font-size: 28rpx;
color: #666867;
line-height: 55rpx;
margin-left: 25rpx;
position: relative;
}
.bottomBox .bottomSum .bshare .bText button{
  width:100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0;
}
.bottomBox .bottomSum .bshare .bJt{
    width: 20rpx;
    height: 20rpx;
    border-top: 2rpx solid #b4b4b6;
    border-right: 2rpx solid #b4b4b6;
    transform: rotate(45deg);
}
